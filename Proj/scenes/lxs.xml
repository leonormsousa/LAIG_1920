<lxs>
    <scene root="root" axis_length="5"/>

    <views default="homerCamera">
        <perspective id="homerCamera" near="0.1" far="500" angle="0">
            <from x="0" y="40" z="30" />
            <to x="0" y="20" z="5" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="homerLight" enabled="1">
            <location x="0" y="25" z="20" w="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="0" linear="1" quadratic="0" />
        </omni>
        <!-- <spot id="spotlight" enabled="1" angle="20" exponent="10">
            <location x="0" y="40" z="-10" w="1.0" />
            <target x="0" y="20" z="5" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="0" linear="1" quadratic="0" />
        </spot> -->
    </lights>

    <textures>
        <texture id="donutTexture" file="scenes/images/donut.jpg" />
        <texture id="paintingTexture" file="scenes/images/painting.png" />
