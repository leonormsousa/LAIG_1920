        <primitive id="rectangle">
            <rectangle x1="-1" y1="-0.5" x2="1" y2="0.5"/>
        </primitive>
        <primitive id="triangle">
            <triangle x1="0" y1="0" z1="0" x2="0" y2="1" z2="0" x3="1" y3="0" z3="0"/>
        </primitive>
        <primitive id="torus">
            <torus inner="0.75" outer="1.2" slices="40" loops="40"/>
        </primitive>
        <primitive id="torus_thin">
            <torus inner="0.08" outer="0.6" slices="40" loops="40"/>
        </primitive>
        <primitive id="torus_thick">
            <torus inner="0.15" outer="0.3" slices="40" loops="40"/>
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="40" stacks="20"/>
        </primitive>
        <primitive id="straightCylinder">
            <cylinder base="1" top="1" height="1" slices="50" stacks="30"/>
        </primitive>
        <primitive id="cylinder_head">
